{
   "action": {
      "default_icon": {
         "128": "icons/icon128.png",
         "16": "icons/icon16.png",
         "32": "icons/icon32.png",
         "48": "icons/icon48.png",
         "64": "icons/icon64.png"
      },
      "default_popup": "popup.html"
   },
   "author": "sublimetrix",
   "background": {
      "service_worker": "service_worker.js"
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "The easiest way to debug and check the dataLayer implementations without having to use the browser console!",
   "host_permissions": [ "http://*/*", "https://*/*", "https://umami.datalayerchecker.com/*" ],
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "32": "icons/icon48.png",
      "48": "icons/icon48.png",
      "64": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Datalayer Checker",
   "permissions": [ "activeTab", "storage" ],
   "short_name": "dlc",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.0.4",
   "web_accessible_resources": [ {
      "matches": [ "http://*/*", "https://*/*" ],
      "resources": [ "js/datalayer-checker.js" ]
   } ]
}
